    "keycloak-js": "20.0.5",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.7",
    "@catena-x/portal-shared-components": {
      "@mui/x-date-pickers": "6.16.1",
      "@mui/types": "7.2.5",
      "@mui/system": {
        "@mui/types": "7.2.5"
      }
    },
    "i18next": "22.5.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-simple-maps|d3-[^/]+)/)"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
