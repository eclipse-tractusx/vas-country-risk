{
  "name": "dashboard-app",
  "version": "1.2.1",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "axios": "1.6.1",
    "cx-portal-shared-components": "^0.4.5",
    "d3-fetch": "^3.0.1",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "html-to-image": "^1.11.11",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "json5": "^2.2.3",
    "jspdf": "^2.5.1",
    "keycloak-js": "22.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-simple-maps": "^3.0.0",
    "react-svg-worldmap": "^2.0.0-alpha.16",
    "react-test-renderer": "^18.2.0",
    "react-tooltip": "4.2.21",
    "web-vitals": "^3.5.0",
    "webpack": "5.89.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f .env.development npm start",
    "start:prod": "env-cmd -f .env.production npm start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:prod": "env-cmd -f .env.production npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --env=jsdom --coverage --collectCoverageFrom='src/**/*.js' ",
    "local-coverage": "react-scripts test --env=jsdom --watchAll --coverage --collectCoverageFrom='src/**/*.js' "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "babel-jest": "^29.7.0",
    "jsdom": "22.0.0",
    "sass": "^1.69.5"
  },
  "overrides": {
    "react-simple-maps": {
      "d3-color": "^3.1.0",
      "d3-zoom": "^3.0.0"
    },
    "react-scripts": {
      "@svgr/webpack": "7.0.0"
    },
    "resolve-url-loader": {
      "postcss": "^8.4.31"
    },
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.7"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-simple-maps|d3-[^/]+)/)"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}
